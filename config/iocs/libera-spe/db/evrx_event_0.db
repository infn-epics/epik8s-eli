
record(aSub, "$(P):$(B):event") 
{
     info(MCII,  "mcii://$(D)/stream/boards.$(MB).signals.event/$(MB):10028/data/0")
    field(INAM,  "asub_signal_init")
    field(SNAM,  "asub_signal_process")
    field(SCAN,  "Passive")
    field(FTA,   "STRING")
    field(NOA,   "$(N)")
    field(FTVA,  "STRING")
    field(NOVA,  "$(N)")
    field(OUTA,  "")
    field(FTVB,  "STRING")
    field(NOVB,  "$(N)")
    field(OUTB,  "")
    field(FTVC,  "STRING")
    field(NOVC,  "$(N)")
    field(OUTC,  "")
    field(FTVD,  "STRING")
    field(NOVD,  "$(N)")
    field(OUTD,  "")
}

record(bo, "$(P):$(B):event:enable") 
{
     info(MCII,  "mcii://$(D)/stream/boards.$(MB).signals.event/$(MB):10028/enable/0")
    field(OUT,   "@asyn($(D),0)$(MB):10028/enable/0")
    field(DTYP,  "asynInt32")
    field(SCAN,  "Passive")
    field(FLNK,  "")
    field(ZNAM,  "NO")
    field(ONAM,  "YES")
}


record(aSub, "$(P):$(B):evrx") 
{
     info(MCII,  "mcii://$(D)/stream/boards.$(MB).signals.evrx/$(MB):10029/data/0")
    field(INAM,  "asub_signal_init")
    field(SNAM,  "asub_signal_process")
    field(SCAN,  "Passive")
    field(FTA,   "STRING")
    field(NOA,   "$(N)")
    field(FTVA,  "STRING")
    field(NOVA,  "$(N)")
    field(OUTA,  "")
    field(FTVB,  "STRING")
    field(NOVB,  "$(N)")
    field(OUTB,  "")
}

record(bo, "$(P):$(B):evrx:enable") 
{
     info(MCII,  "mcii://$(D)/stream/boards.$(MB).signals.evrx/$(MB):10029/enable/0")
    field(OUT,   "@asyn($(D),0)$(MB):10029/enable/0")
    field(DTYP,  "asynInt32")
    field(SCAN,  "Passive")
    field(FLNK,  "")
    field(ZNAM,  "NO")
    field(ONAM,  "YES")
}
